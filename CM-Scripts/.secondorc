# secondo-bashrc executed at startup of a bash
#
# Copy this file into your $HOME directory and add
# the line 
#
# source ./secondo-bashrc
#
# at the end of file $HOME/.bashrc
#
# DO NOT CHANGE THIS FILE unless you know what you are doing!

if [ "$OSTYPE" = "msys" ]; then
   SECONDO_PREFIX=""
else 
   SECONDO_PREFIX="$HOME"
fi

export SECONDO_SDK="$SECONDO_PREFIX/secondo-sdk"

alias setvar='source $SECONDO_SDK/bin/setvar.bash' 
alias catvar='$SECONDO_SDK/bin/catvar.sh' 
alias jcvs='java -jar /home/secondo/secondo-sdk/jCVS-5.2.2/jars/jcvsii.jar'
alias cvs-info='cvs -nq update | grep "[A-Z]"'

source $SECONDO_SDK/bin/setvar.bash $HOME/secondo

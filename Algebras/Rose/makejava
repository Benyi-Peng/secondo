#
# last change: 2003-07-03; Mirco Guenster

include makefile.inc

.PHONY:all
all: 
	(cd RoseJava; make all)

.PHONY: clean
clean: 
	(cd RoseJava; make clean)
	rm -fr $(DOCPATH)


.PHONY: doc
doc:
	mkdir $(DOCPATH)
	$(JAVADOC) -d $(DOCPATH) -classpath "$(CLASSPATH)" $(PACKAGES)

# compile java files
%.class:%.java
	$(JAVAC) -classpath $(CLASSPATH) $<








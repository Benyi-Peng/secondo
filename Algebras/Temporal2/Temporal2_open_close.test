delete database mytestdb;
delete database mytestdb2;

#setup Forall2Test StandardAlgebra RelationAlgebra Temporal2Algebra
create database mytestdb;
create database mytestdb2;

# Here come: The tests

# ================
# Real Tests
# ================

# ==============================================================================
# TypeConstructor MMPoint
# ==============================================================================

# ----------------------
# -- anonymous object --
# ----------------------

#testcase open_db_1st_time
#yields success
open database mytestdb;

# --------------------------------
# -- variable assignement - let --
# --------------------------------

#testcase mmpoint_variable_undef_1
#yields success
let mm_var_undef = [const mmpoint value undef];

#testcase mmpoint_variable_2000_1
#yields success
let mm_var_2000 = [const mmpoint value ((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0)))];

#testcase mmpoint_variable_2000_2100_1
#yields success
let mm_var_2000_2100 = [const mmpoint value ((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))(("2100-01-01" "2100-01-01-00:01" TRUE FALSE)(1.0 1.0 0.0 0.0)))];

# ---------------------------
# -- attribute in relation --
# ---------------------------

#testcase mmpoint_relation_undef_1
#yields success
let mm_rel_undef = [const rel(tuple([MMP: mmpoint])) value ((undef))];

#testcase mmpoint_relation_2000_1
#yields success
let mm_rel_2000 = [const rel(tuple([MMP: mmpoint])) value (( ((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))) ))];

#testcase mmpoint_relation_2000_2100_1
#yields success
let mm_rel_2000_2100 = [const rel(tuple([MMP: mmpoint])) value ((((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))(("2100-01-01" "2100-01-01-00:01" TRUE FALSE)(1.0 1.0 0.0 0.0)))))];

# -----------------------------
# -- close and open database --
# -----------------------------

#testcase closeandopen_db_1
#yields success
close database;

#testcase closeandopen_db_2
#yields success
open database mytestdb;

# ------------------------------------------------------
# -- variable assignement - let - check after re-open --
# ------------------------------------------------------

#testcase mmpoint_variable_undef_2
#yields (mmpoint undefined)
query mm_var_undef;

#testcase mmpoint_variable_2000_2
#yields (mmpoint((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))))
query mm_var_2000;

#testcase mmpoint_variable_2000_2100_2
#yields (mmpoint((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))(("2100-01-01" "2100-01-01-00:01" TRUE FALSE)(1.0 1.0 0.0 0.0))))
query mm_var_2000_2100;


# -------------------------------------------------
# -- attribute in relation - check after re-open --
# -------------------------------------------------

#testcase mmpoint_relation_undef_2
#yields ((rel (tuple ((MMP mmpoint)))) ((undefined)))
query mm_rel_undef;

#testcase mmpoint_relation_2000_2
#yields ((rel(tuple((MMP mmpoint))))((((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))))))
query mm_rel_2000;

#testcase mmpoint_relation_2000_2100_2
#yields ((rel(tuple((MMP mmpoint))))((((("2000-01-01" "2000-01-01-00:01" TRUE FALSE)(0.0 0.0 1.0 1.0))(("2100-01-01" "2100-01-01-00:01" TRUE FALSE)(1.0 1.0 0.0 0.0))))))
query mm_rel_2000_2100;

#testcase mmpoint_relation_invalid
#yields error
let mm_rel_invalid = [const rel(tuple([MMP: mmpoint])) value (( 123 ))];


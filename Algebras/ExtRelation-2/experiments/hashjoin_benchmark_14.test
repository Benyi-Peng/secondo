#--------------------------------------------------  
# Enable Flags
#--------------------------------------------------  

set "ERA:TraceHybridHashJoin" = TRUE;
#set "SI:PrintCounters" = TRUE;
set "SI:PrintCmdTimes" = TRUE;
set "SI:NoQueryTimers" = TRUE;
set "SI:NoCommandEcho" = TRUE;
#set "SI:RelStatistics" = TRUE;
set "SMI:NoTransactions" = TRUE;

#--------------------------------------------------  
# Create and open database DB_TMP
#--------------------------------------------------  

#setup hashjoin_benchmark StandardAlgebra RelationAlgebra ExtRelation2Algebra

create database db_tmp;
open database db_tmp;

#--------------------------------------------------  
# Benchmark with 3072 KByte Memory, 64 Byte tuples
#--------------------------------------------------  

#testcase create_relation
#yields success
restore R50MB_T64B from '$(SECONDO_BUILD_DIR)/Algebras/ExtRelation-2/experiments/R50MB_T64B';

#testcase create_relation_A (50 MB)
#yields success
let relA = R50MB_T64B feed {a} consume;

#testcase create_relation_B (10 MB)
#yields success
let relB = R50MB_T64B feed head[81920] {b} consume;

#testcase sortmergejoin_M3072_T64
#yields success
query relA feed relB feed sortmergejoin[Flob_a,Flob_b] count;

#testcase hashjoin_M3072_T64
#yields success
query relA feed relB feed hashjoin[Flob_a,Flob_b;1000] count;

#testcase sortmergejoin_M3072_T64
#yields success
query relA feed relB feed sortmergejoin2[Flob_a,Flob_b] count;

#testcase gracehashjoin_M3072_T64
#yields success
query relA feed relB feed gracehashjoin[Flob_a,Flob_b;1000] count;

#testcase hybridhashjoin_M3072_T64
#yields success
query relA feed relB feed hybridhashjoin[Flob_a,Flob_b;1000] count;

#testcase delete_relation_A
#yields success
delete relA;

#testcase delete_relation_B
#yields success
delete relB;

#testcase delete_relation_B
#yields success
delete R50MB_T64B;


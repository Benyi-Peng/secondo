##############################################################################
# Setup
##############################################################################

#setup ExtRelation2test StandardAlgebra RelationAlgebra ExtRelation2Algebra

open database berlintest;

##############################################################################
# TestCases
##############################################################################

#testcase hybridhashjoin_success1
#yields @./../Algebras/ExtRelation-2/Tests/hashjoin.result1
query Orte feed {o} plz feed {p} hybridhashjoin[Ort_o, Ort_p, 1000] sort2 consume;

#testcase hybridhashjoin_success2
#yields @./../Algebras/ExtRelation-2/Tests/hashjoin.result1
query Orte feed {o} plz feed {p} hybridhashjoin[Ort_o, Ort_p, 0] sort2 consume;

#testcase hybridhashjoin_success3
#yields @./../Algebras/ExtRelation-2/Tests/hashjoin.result1
query Orte feed {o} plz feed {p} hybridhashjoin[Ort_o, Ort_p, 20000] sort2 consume;

#testcase hybridhashjoin_error1
#yields error
query Orte feed {o} plz feed {p} hybridhashjoin[Ort_o, Ort_p] consume;

#testcase hybridhashjoin_error2
#yields error
query Orte feed {o} plz feed {p} hybridhashjoin[Ort_o, Ort_p, 1000, 32] consume;

#testcase hybridhashjoinP_success1
#yields @./../Algebras/ExtRelation-2/Tests/hashjoin.result1
query Orte feed {o} plz feed {p} hybridhashjoinP[Ort_o, Ort_p, 1000, 32, 16*1024*1024, 4096] sort2 consume;

#testcase hybridhashjoinP_success2
#yields @./../Algebras/ExtRelation-2/Tests/hashjoin.result1
query Orte feed {o} plz feed {p} hybridhashjoinP[Ort_o, Ort_p, 1000, 32, 4096, 4096] sort2 consume;

#testcase hybridhashjoinP_success3
#yields @./../Algebras/ExtRelation-2/Tests/hashjoin.result1
query Orte feed {o} plz feed {p} hybridhashjoinP[Ort_o, Ort_p, 0, 0, 0, 0] sort2 consume;

#testcase hybridhashjoinP_error1
#yields error
query Orte feed {o} plz feed {p} hybridhashjoinP[Ort_o, Ort_p, 0, 0, 0] consume;


##############################################################################
# TearDown
##############################################################################

#teardown
close database;


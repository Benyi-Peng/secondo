##############################################################################
# Setup
##############################################################################

#setup ExtRelation2test StandardAlgebra RelationAlgebra ExtRelation2Algebra

restore database tmp_opt from '$(SECONDO_BUILD_DIR)/bin/opt' 

##############################################################################
# TestCases
##############################################################################

#testcase teststep1_create_result
#yields success
let hashjoin_result1 = Orte feed {o} plz feed {p} hashjoin[Ort_o, Ort_p, 1000] sort consume;

#testcase teststep1_compare_result
#yields *hashjoin_result1
query Orte feed {o} plz feed {p} hashjoin[Ort_o, Ort_p, 1000] sort consume;

#--------------------------------------------------  

#testcase teststep1_compare_result
#yields *hashjoin_result1
query Orte feed {o} plz feed {p} hybridhashjoin[Ort_o, Ort_p, 1000] sort consume;

##############################################################################
# TearDown
##############################################################################

#teardown
close database;
delete database tmp_opt;

#/bin/bash

if [ -z "$1" ]; then
  echo "Missing Server Configuration File"
  exit 1
fi

config=$1
action=$2

start='DBService'

function startLine {

  if [ -z "$1" ]; then  # empty line
    return
  fi
  if [ -z "$2" ]; then  # empty line
    echo "invalid format in line" $*
    return
  fi

  server=$1
  config=$2

  if [ -n "$5" ]; then
    if [ "$5" != "$" ]; then
       port=$5
    fi
  fi

  echo $server
  echo $config
  echo $port

}

if [ $action == start ]; then
   while read line; do startLine $line ;done <$1
fi
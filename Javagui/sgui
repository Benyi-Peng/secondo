if [ "$SECONDO_PLATFORM" = "win32" ]
    then S=";"
    else S=":"
fi    

CP="lib/java_cup_v10k_runtime.jar"$S"lib/jl0.4.jar"$S"."$S"lib/14_os_jpedal.jar""$S"lib/je.jar$S"lib/grappa1_2.jar"

if [ -n "$SECONDO_JAVA" ]; then
  JAVA=$SECONDO_JAVA
  CLASSPATH=$SECONDO_JAVART
else 
  JAVA="java"
fi

$JAVA  -Xmx1024M -Djava.library.path=lib  -classpath $CP gui.MainWindow $*
